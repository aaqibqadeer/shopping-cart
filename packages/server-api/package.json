{
  "name": "@bootcamp/server",
  "version": "1.0.0",
  "description": "This is a practice project.",
  "main": "index.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon --delay 500ms --watch 'src/**' --ext 'ts,json' --exec 'ts-node' src/index.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  },
  "watch": [
    "src/"
  ],
  "execMap": {
    "ts": "node -r ts-node/register"
  }
}